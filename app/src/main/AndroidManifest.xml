<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jerry.dynaviewer.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jerry.dynaviewer.app.CardListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jerry.dynaviewer.app.CardDetailActivity"
            android:label="@string/title_card_detail"
            android:parentActivityName="com.jerry.dynaviewer.app.CardListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jerry.dynaviewer.app.CardListActivity" />
        </activity>
    </application>

</manifest>
